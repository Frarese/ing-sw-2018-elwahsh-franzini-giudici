<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<VBox id="container" stylesheets="/it/polimi/se2018/view/app/fxmlFiles/cssFiles/standardCSS.css" alignment="CENTER"
      prefHeight="385.0" prefWidth="309.0" spacing="10.0" xmlns="http://javafx.com/javafx/8"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="it.polimi.se2018.view.tools.fx.controller.ShowInvitesController">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
    <HBox alignment="CENTER">
        <Label text="INVITI">
            <font>
                <Font name="System Bold" size="36.0"/>
            </font>
        </Label>
    </HBox>
    <TableView fx:id="table" prefHeight="200.0" prefWidth="316.0">
        <columns>
            <TableColumn prefWidth="288.0" text="Identificatore"/>
        </columns>
    </TableView>
    <HBox alignment="CENTER" spacing="5.0">
        <Button defaultButton="true" mnemonicParsing="false" onAction="#close" text="Indietro"/>
        <Button mnemonicParsing="false" onAction="#acceptInvite" text="Accetta"/>
    </HBox>
</VBox>
